{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devPath": "http://localhost:4200",
    "distDir": "../../dist/apps/frontend",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "musicnya",
    "version": "1.0.0-alpha.0"
  },
  "tauri": {
    "allowlist": {
      "all": true,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "musicnya",
        "width": 830,
        "height": 700,
        "minHeight": 700,
        "minWidth": 830,
        "decorations": false
      }
    ]
  }
}
